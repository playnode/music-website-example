    "webpack": "webpack",
    "webpack:production": "webpack --env.production",
    "webpack:watch": "webpack --watch",
    "clean:all": "trash public/dist es5",
    "clean:es5": "trash es5"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
