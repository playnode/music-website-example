{
  "private": true,
  "name": "playnode-music-website",
  "version": "0.0.1",
  "dependencies": {
    "@playnode/react-music-components": "^0.0.1-alpha.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "file-loader": "^1.1.6",
    "http-server": "^13.0.2",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "trash-cli": "^4.0.0",
    "webpack": "^5.54.0",
    "webpack-cli": "^3.3.12"
  },
  "scripts": {
    "quick-start": "npm-run-all build --parallel serve build:watch",
    "build": "run-s clean:all webpack",
